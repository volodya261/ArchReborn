[
  {
    "modules-center": [
      "hyprland/workspaces"
    ],
    "modules-left": [
      "custom/cpuinfo",
      "custom/mem",
      "idle_inhibitor"
    ],
    "modules-right": [
      "hyprland/language",
      "backlight",
      "pulseaudio",
      "battery",
      "clock",
      "tray"
    ],
    "position": "top",
    "layer": "bottom",
    "output": "eDP-1",
